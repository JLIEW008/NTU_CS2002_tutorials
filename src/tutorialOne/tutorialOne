		Attribute numberOfLecture;
		Attribute numberOfTutorial;
		Attribute ArrayList<Lecture>;
		Attribute ArrayList<Tutorial>;
	
		Behaviour addLecture
		Behaviour dropLecture
		Behaviour static getNumberOfLecture
		Behaviour addTutorial
		Behaviour dropTutoiral
		Behaviour static getNumberOfTutorial
		Behaviour updateResults
	
	Class AdminStaff extend Staff
	
		Behaviour generateLectureSchedule
		Behaviour editLectureSchedule
		Behaviour generateTutorialSchedule
		Behaviour editTutoiralSchedule
		Behaviour doAdminStuff
	
